{
  "author": "Haneru Developers",
  "babel": {
    "presets": [
      "next/babel",
      "@zeit/next-typescript/babel"
    ]
  },
  "dependencies": {
    "@zeit/next-typescript": "^1.1.1",
    "algoliasearch": "^3.33.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-alpha.36",
    "next": "^8.1.0",
    "next-offline": "^4.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-list": "^0.8.11"
  },
  "description": "",
  "devDependencies": {
    "@inabagumi/prettier-config": "^1.0.0",
    "@types/algoliasearch": "^3.30.12",
    "@types/classnames": "^2.2.8",
    "@types/next": "^8.0.5",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-list": "^0.8.5",
    "@types/styled-jsx": "^2.2.8",
    "@types/yargs": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "googleapis": "^40.0.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "typescript": "~3.5.2",
    "yargs": "^13.2.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier/@typescript-eslint",
      "prettier/react"
    ],
    "overrides": [
      {
        "files": [
          "next.config.js"
        ],
        "rules": {
          "@typescript-eslint/no-var-requires": "off"
        }
      }
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-member-accessibility": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "react/prop-types": "off",
      "react-hooks/exhaustive-deps": "error",
      "react-hooks/rules-of-hooks": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "license": "MIT",
  "name": "@inabagumi/animare-search",
  "prettier": "@inabagumi/prettier-config",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,json,ts,tsx}\"",
    "indexing": "ts-node --project scripts/tsconfig.json scripts/indexing.ts",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "start": "next start"
  },
  "version": "1.0.0"
}
