{
  "author": "Haneru Developers",
  "description": "SHINJU DATE is a web service for searching videos.",
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "supabase": "2.65.6",
    "turbo": "2.6.3",
    "typescript": "5.9.3"
  },
  "homepage": "https://shinju.date/",
  "license": "MIT",
  "name": "@shinju-date/monorepo",
  "packageManager": "pnpm@10.25.0",
  "pnpm": {
    "neverBuiltDependencies": [
      "@biome/biome"
    ]
  },
  "private": true,
  "scripts": {
    "build": "turbo build",
    "check": "biome check",
    "db:export": "./scripts/export-production-data.sh",
    "db:schema": "supabase gen types typescript --local --schema public > packages/database/types/supabase.ts",
    "db:import": "./scripts/import-data.sh",
    "dev": "turbo dev",
    "test": "turbo test",
    "test:e2e": "turbo test:e2e",
    "typegen": "turbo typegen"
  },
  "type": "module",
  "version": "1.0.0"
}
