{
  "customizations": {
    "vscode": {
      "extensions": [
        "EditorConfig.EditorConfig",
        "biomejs.biome",
        "bradlc.vscode-tailwindcss",
        "unifiedjs.vscode-mdx",
        "ms-python.python",
        "ms-python.ruff"
      ],
      "settings": {}
    }
  },
  "dockerComposeFile": ["../compose.yml", "compose.override.yml"],
  "features": {
    "ghcr.io/devcontainers/features/python:1": {},
    "ghcr.io/devcontainers/features/terraform:1": {
      "terragrunt": "none",
      "tflint": "none",
      "version": "1.6.4"
    }
  },
  "forwardPorts": [3000, 4000, 8000, 8079, 10000, 54321, 54323, 54324, 54327],
  "hostRequirements": {
    "cpus": 4,
    "memory": "8gb",
    "storage": "32gb"
  },
  "name": "SHINJU DATE",
  "portsAttributes": {
    "3000": {
      "label": "Web",
      "onAutoForward": "silent"
    },
    "4000": {
      "label": "Admin",
      "onAutoForward": "silent"
    },
    "8000": {
      "label": "Insights API",
      "onAutoForward": "silent"
    },
    "8079": {
      "label": "Redis HTTP API",
      "onAutoForward": "silent"
    },
    "10000": {
      "label": "Batch",
      "onAutoForward": "silent"
    },
    "54321": {
      "label": "Supabase API",
      "onAutoForward": "silent"
    },
    "54323": {
      "label": "Supabase Studio",
      "onAutoForward": "notify"
    },
    "54324": {
      "label": "Mailpit Web UI",
      "onAutoForward": "notify"
    },
    "54327": {
      "label": "Supabase Analytics",
      "onAutoForward": "silent"
    }
  },
  "postCreateCommand": "./.devcontainer/post-create.sh",
  "service": "app",
  "waitFor": "postCreateCommand",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}"
}
