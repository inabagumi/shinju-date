NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Supabase client (using local Supabase instance)
# Start Supabase with: pnpm exec supabase start
# Then get keys with: pnpm exec supabase status -o env
NEXT_PUBLIC_SUPABASE_ANON_KEY=""
NEXT_PUBLIC_SUPABASE_URL="http://localhost:54321"

# Upstash Redis (using local serverless-redis-http)
# In Dev Container: automatically started
# Otherwise start with: cd .devcontainer && docker compose up -d
UPSTASH_REDIS_REST_TOKEN="local_development_token"
UPSTASH_REDIS_REST_URL="http://localhost:8079"

# Optional: Enable MSW for testing without local services
# Set ENABLE_MSW=true to use mocks instead of real services
# ENABLE_MSW=true

# Resend API configuration for contact form (optional)
RESEND_API_KEY=
FROM_EMAIL=noreply@shinju.date
ADMIN_EMAIL=admin@example.com
