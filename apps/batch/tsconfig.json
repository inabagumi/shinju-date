{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": ["./*"]
    },
    "skipLibCheck": true,
    "target": "ESNext"
  },
  "extends": ["@tsconfig/strictest/tsconfig"],
  "include": [".nitro/types/**/*.ts", "**/*.ts", "server/**/*.ts"]
}
