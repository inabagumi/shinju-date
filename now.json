{
  "build": {
    "env": {
      "ALGOLIA_API_KEY": "@algolia-api-key",
      "ALGOLIA_APPLICATION_ID": "@algolia-application-id",
      "ALGOLIA_INDEX_NAME": "prod_ANIMARE_SEARCH",
      "SHINJU_DATE_BASE_URL": "https://shinju.date",
      "SHINJU_DATE_DESCRIPTION": "『SHINJU DATE』は有閑喫茶 あにまーれ、​ハニーストラップ、ブイアパのメンバーがYouTubeで配信した放送や投稿した動画の検索ができるウェブサービスです。コラボ放送の検索などにご活用ください。",
      "SHINJU_DATE_TITLE": "SHINJU DATE"
    }
  },
  "env": {
    "NODE_ICU_DATA": "node_modules/full-icu"
  },
  "github": {
    "silent": true
  },
  "name": "animare-search",
  "regions": ["hnd1"],
  "routes": [
    {
      "continue": true,
      "headers": {
        "cache-control": "max-age=600, s-maxage=120"
      },
      "src": ".*"
    },
    {
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "max-age=0"
      },
      "src": "/service-worker\\.js"
    },
    {
      "headers": {
        "cache-control": "immutable, max-age=31536000"
      },
      "src": "/_next/.+"
    },
    {
      "continue": true,
      "headers": {
        "content-security-policy": "connect-src 'self' https://*.algolia.net https://*.algolianet.com https://fonts.googleapis.com https://fonts.gstatic.com; default-src 'none'; font-src https://fonts.gstatic.com; img-src 'self' data: https://i.ytimg.com; manifest-src 'self'; script-src 'self' 'unsafe-inline' https://storage.googleapis.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; worker-src 'self'"
      },
      "src": ".*"
    },
    {
      "headers": {
        "cache-control": "max-age=300,s-maxage=60"
      },
      "src": "/(?:search)?"
    },
    {
      "handle": "filesystem"
    },
    {
      "dest": "/_error",
      "headers": {
        "cache-control": "max-age=0"
      },
      "src": ".*"
    }
  ],
  "scope": "inabagumi",
  "version": 2
}
